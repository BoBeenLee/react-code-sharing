{
  "name": "kr.rnapp.desktop",
  "version": "1.0.0",
  "productName": "desktop",
  "description": "rndesktop",
  "main": ".webpack/main",
  "scripts": {
    "start": "NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages -o ./storybook-static -c .storybook",
    "prettier": "prettier './src/**/*.{js,ts,tsx}'",
    "eslint": "./node_modules/.bin/eslint './src/**/*.{js,ts,tsx}'",
    "eslint:fix": "./node_modules/.bin/eslint './src/**/*.{js,ts,tsx}' --fix",
    "prepush": "npm run eslint",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "version": "npm run changelog",
    "release": "release-it --ci"
  },
  "keywords": [],
  "author": {
    "name": "bobinlee",
    "email": "bobeen.lee@huiseoul.com"
  },
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^7.5.2",
    "framer-motion": "^1.7.0",
    "hoist-non-react-statics": "^3.3.0",
    "mobile-detect": "^1.4.4",
    "mobx": "^4.15.4",
    "mobx-react": "^6.2.2",
    "mobx-state-tree": "^3.16.0",
    "qs": "^6.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-responsive": "^8.0.3",
    "react-sizeme": "^2.6.12",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "recompose": "^0.30.0",
    "styled-components": "^5.1.1",
    "styled-theming": "^2.2.0",
    "uuid": "^3.3.3",
    "react-native": "0.62.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.49",
    "@electron-forge/maker-deb": "^6.0.0-beta.49",
    "@electron-forge/maker-rpm": "^6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.49",
    "@electron-forge/maker-zip": "^6.0.0-beta.49",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.49",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/preset-flow": "^7.0.0",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.23",
    "@types/qs": "^6.9.0",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/react-responsive": "^3.0.3",
    "@types/react-window": "^1.8.1",
    "@types/react-window-infinite-loader": "^1.0.2",
    "@types/recompose": "^0.30.7",
    "@types/styled-components": "^5.1.1",
    "@types/styled-theming": "^2.2.2",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "css-loader": "^3.4.2",
    "auto-changelog": "^1.16.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-native-web": "^0.11.7",
    "babel-preset-react-app": "^9.1.0",
    "electron": "9.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "reactotron-mst": "^3.1.3",
    "reactotron-react-js": "^3.3.7",
    "release-it": "^12.4.3",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.6"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "desktop"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  }
}
