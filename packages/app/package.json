  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:reset": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "jest",
    "android": "npm run android:remove || node node_modules/react-native/local-cli/cli.js run-android",
    "android:log": "adb logcat '*:S' ReactNative:V ReactNativeJS:V",
    "android:release": "npm run android:remove || node node_modules/react-native/local-cli/cli.js run-android --variant=release",
    "android:release:open": "open ./android/app/build/outputs/apk",
    "android:aab": "./android && ./gradlew clean && ./gradlew bundleRelease",
    "android:aab:open": "open ./android/app/build/outputs/bundle/release",
    "android:remove": "adb uninstall kr.rnapp.app",
    "android-bugsnag": "npm run android-bundle-release && npm run android-bugsnag-upload",
    "android-bundle-release": "REACT_ENV=production react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android-release.bundle --sourcemap-output android-release.bundle.map",
    "android-bugsnag-upload": "bugsnag-sourcemaps upload --api-key 0bda7c3b99b638a82d9a74c9872e0b79 --minified-file android-release.bundle --overwrite true --source-map android-release.bundle.map --minified-url index.android.bundle --upload-sources",
    "ios:reset": "rm -rf ios/build/ModuleCache",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone 11'",
    "ios6": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone 6s Plus'",
    "ios7": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone 7 Plus'",
    "ios8": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone 8'",
    "ios:release": "node node_modules/react-native/local-cli/cli.js run-ios --configuration Release",
    "ios:devices": "xcrun simctl list devices",
    "ios-bugsnag": "npm run ios-bundle-release && npm run ios-bugsnag-upload",
    "ios-bundle-release": "REACT_ENV=production react-native bundle --platform ios --dev false --entry-file index.ios.js --bundle-output ios-release.bundle --sourcemap-output ios-release.bundle.map",
    "ios-bugsnag-upload": "bugsnag-sourcemaps upload --api-key 0bda7c3b99b638a82d9a74c9872e0b79 --minified-file ios-release.bundle --overwrite true --source-map ios-release.bundle.map --minified-url main.jsbundle --upload-sources",
    "build:ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='index.ios.js' --bundle-output='./ios/RNApp/main.jsbundle' --dev=false --platform='ios'",
    "build:android": "cd ./android && ./gradlew clean && ./gradlew assembleRelease",
    "appdistribution:android": "npm run build:android && firebase appdistribution:distribute ./android/app/build/outputs/apk/release/app-release.apk --app 1:729865146079:android:1ec7739544849fc8 --token 1/Imdv27tKHLaob8Y1o4h6CA6-kn_zbXZXQi6e_-Xfuus",
    "appdistribution:ios": "firebase appdistribution:distribute $BITRISE_IPA_PATH --app 1:833905792974:ios:1ec7739544849fc8 --token 1/Imdv27tKHLaob8Y1o4h6CA6-kn_zbXZXQi6e_-Xfuus",
    "codepush:android:production": "code-push release-react onesecondmusicgame/onesecondmusicgame-android android -d Production -m true --privateKeyPath ~/private.pem",
    "codepush:ios:production": "code-push release-react onesecondmusicgame/onesecondmusicgame-ios ios -d Production -m true --privateKeyPath ~/private.pem --plistFile ios/RNApp/Info.plist",
    "fastlane:ios-ci": "fastlane ios ci",
    "fastlane:android-ci": "fastlane android ci",
    "prettier": "prettier './src/**/*.{js,ts,tsx}'",
    "eslint": "./node_modules/.bin/eslint './src/**/*.{js,ts,tsx}'",
    "eslint:fix": "./node_modules/.bin/eslint './src/**/*.{js,ts,tsx}' --fix",
    "type-check": "tsc --noEmit",
    "xcode": "open ios/RNApp.xcworkspace",
    "androidstudio": "open -a /Applications/Android\\ Studio.app ./android",
    "androidrectotronconnect": "adb reverse tcp:9090 tcp:9090",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "storybook:android:connect": "adb reverse tcp:7007 tcp:7007",
    "prepush": "npm run eslint",
    "postinstall": "npx jetify",
    "postbuildnumber:ios": "RNV=ios react-native-version",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "version": "npm run changelog && react-native-version --reset-build",
    "release": "release-it --ci",
    "bundlesize": "react-native-bundle-visualizer --entry-file ./index.ios.js"
  },
