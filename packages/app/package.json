{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 7007",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "ios6": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPhone 6'",
    "ios:release": "REACT_ENV=production ./appcenter-pre-build.sh && react-native run-ios --configuration Release",
    "ios-bugsnag": "npm run ios-bundle-release && npm run ios-bugsnag-upload",
    "ios-bundle-release": "REACT_ENV=production ./appcenter-pre-build.sh && react-native bundle --platform ios --dev false --entry-file index.ios.js --bundle-output ios-release.bundle --sourcemap-output ios-release.bundle.map",
    "ios-bugsnag-upload": "bugsnag-sourcemaps upload --api-key f4b104f0023813381c0b7ab9f1d3c583 --minified-file ios-release.bundle --overwrite true --source-map ios-release.bundle.map --minified-url main.jsbundle --upload-sources",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "android:release": "REACT_ENV=production ./appcenter-pre-build.sh && react-native run-android --variant=release && open ./android/app/build/outputs/apk",
    "android:staging": "REACT_ENV=staging ./appcenter-pre-build.sh && react-native run-android --variant=release && open ./android/app/build/outputs/apk",
    "android-bugsnag": "npm run android-bundle-release && npm run android-bugsnag-upload",
    "android-bundle-release": "REACT_ENV=production ./appcenter-pre-build.sh && react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android-release.bundle --sourcemap-output android-release.bundle.map",
    "android-bugsnag-upload": "bugsnag-sourcemaps upload --api-key f4b104f0023813381c0b7ab9f1d3c583 --minified-file android-release.bundle --overwrite true --source-map android-release.bundle.map --minified-url index.android.bundle --upload-sources",
    "build": "tsc",
    "test": "jest",
    "lint": "tslint {src,__tests__,typings}/**/*.{ts,tsx}",
    "lint:css": "stylelint {src,__tests__,typings}/**/*.{ts,tsx}",
    "prepush": "npm run lint && npm run lint:css",
    "playground": "graphql playground"
  },
  "dependencies": {
    "@types/lodash": "^4.14.109",
    "antd-mobile-rn": "^2.2.0",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-persisted-queries": "^0.2.1",
    "aws-sdk": "^2.380.0",
    "bugsnag-react-native": "2.10.1",
    "date-fns": "^1.29.0",
    "formik": "^1.3.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "hoist-non-react-statics": "^2.5.4",
    "lottie-react-native": "^2.5.6",
    "mobx": "^4.8.0",
    "mobx-react": "^5.4.3",
    "mobx-state-tree": "^3.9.0",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "react": "16.6.0",
    "react-native": "0.57.8",
    "react-native-appsflyer": "^1.2.13",
    "react-native-camera": "git+https://github.com/react-native-community/react-native-camera.git#5130aa45cdf31aa4f9ce51a33c19b53c14bdde8c",
    "react-native-code-push": "^5.3.4",
    "react-native-color-matrix-image-filters": "^5.0.1",
    "react-native-device-info": "^0.22.6",
    "react-native-fast-image": "^4.0.14",
    "react-native-firebase": "^5.1.1",
    "react-native-gallery-manager": "^1.0.9",
    "react-native-htmlview": "^0.13.0",
    "react-native-image-crop-picker": "^0.20.3",
    "react-native-image-resizer": "^1.0.0",
    "react-native-image-zoom-viewer": "^2.2.13",
    "react-native-kakao-logins": "^1.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.7.2",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-keychain": "^3.0.0-rc.3",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-navigation": "^2.7.0",
    "react-native-permissions": "^1.1.1",
    "react-native-read-more-text": "^1.1.0",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-splash-screen": "3.0.9",
    "react-native-tab-view": "^1.0.2",
    "recompose": "^0.30.0",
    "rn-fetch-blob": "^0.10.12",
    "styled-components": "^3.3.2",
    "styled-theming": "^2.2.0",
    "typescript": "^3.1.3",
    "uuid": "^3.3.2",
    "yup": "^0.26.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-transform-typescript": "^7.1.0",
    "@babel/runtime": "^7.1.5",
    "@storybook/addon-actions": "^4.0.11",
    "@storybook/addon-links": "^4.0.11",
    "@storybook/addons": "^4.0.11",
    "@storybook/react-native": "^4.0.11",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^23.0.2",
    "@types/node": "^10.3.2",
    "@types/numeral": "0.0.25",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.55.18",
    "@types/react-native-htmlview": "^0.12.1",
    "@types/react-native-permissions": "^1.1.1",
    "@types/react-native-snap-carousel": "^3.6.3",
    "@types/react-native-tab-view": "^1.0.1",
    "@types/react-native-video": "^2.0.4",
    "@types/react-test-renderer": "^16.0.1",
    "@types/recompose": "^0.27.0",
    "@types/yup": "^0.24.7",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "4.0.0",
    "babel-runtime": "^6.26.0",
    "graphql-cli": "^2.17.0",
    "husky": "^0.14.3",
    "jest": "23.1.0",
    "metro-react-native-babel-preset": "^0.49.2",
    "prettier": "^1.14.0",
    "react-dom": "16.4.0",
    "react-test-renderer": "16.4.0",
    "reactotron-mst": "^2.1.0",
    "reactotron-react-native": "^2.1.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^3.6.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
